<div>
  <div class="container">
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Contact</th>
          <th scope="col">Resume</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(u=>{ %>
        <tr>
          <th scope="row"><%= u.id %></th>
          <td><%= u.name %></td>
          <td><%= u.email %></td>
          <td><%= u.contact %></td>
          <td>
            <a
              class="btn btn-sm btn-primary"
              href="/resume/<%= u.resume %>"
              >View</a
            >
          </td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>



